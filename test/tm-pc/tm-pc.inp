Dave Korzekwa, MST6, dak@lanl.gov

Andrew Kuprat, T-1, kuprat@lanl.gov (modification for AIN-solver)

================================================================================

define a three-dimensional mesh

 2 x  3 x 4  cells
 2 x  3 x 4  cm


&MESH
       mesh_file = '233_box.exo',
       coordinate_scale_factor         =  0.01
/

define the solid material geometry and the initial conditions

&BODY

    surface_name         = 'from mesh file'
    material_name        = 'aluminum'
    mesh_material_number = 1
    temperature          = 5.0e+02,

/

define the boundary conditions:

x = 0.0e+00 boundary

&BC

    surface_name     = 'from mesh file',
    mesh_surface          = 1,
    bc_variable      = 'displacement',
    bc_type          = 'x-displacement',
    bc_value         = 0.0

/

&THERMAL_BC
  name = 'x=0'
  type = 'flux'
  face_set_ids = 1
  flux = 0.0
/

x = 2.0e-2 boundary

&THERMAL_BC
  name = 'x=2.0e-2'
  type = 'flux'
  face_set_ids = 2
  flux = 0.0
/

&BC

    surface_name     = 'from mesh file',
    mesh_surface          = 2,
    bc_variable      = 'displacement',
    bc_type          = 'x-displacement',
    bc_value         = 0.0

/

y = 0.0e+00 boundary

&BC

    surface_name     = 'from mesh file',
    mesh_surface          = 3,
    bc_variable      = 'displacement',
    bc_type          = 'y-displacement',
    bc_value         = 0.0e+00

/

&THERMAL_BC
  name = 'y=0'
  type = 'flux'
  face_set_ids = 3
  flux = 0.0
/

y = 3.0e-2 boundary

&THERMAL_BC
  name = 'y=3.0e-2'
  type = 'flux'
  face_set_ids = 4
  flux = 0.0
/

&BC

    surface_name     = 'from mesh file',
    mesh_surface          = 4,
    bc_variable      = 'displacement',
    bc_type          = 'y-displacement',
    bc_value         = 0.0e+00

/

z = 0.0e+00 boundary

&BC

    surface_name     = 'from mesh file',
    mesh_surface          = 5,
    bc_variable      = 'displacement',
    bc_type          = 'z-displacement'
    bc_value         = 0.0e+00

/

&THERMAL_BC
  name = 'z=0'
  type = 'temperature'
  face_set_ids = 5
  temp = 300.0
/

z = 4.0 boundary

&THERMAL_BC
  name = 'z=4'
  type = 'flux'
  face_set_ids = 6
  flux = 0.0
/

define the material properties (aluminum, SI units)
Elastic constants (stresses) in MPa to make test checking easier

&MATERIAL
  name = 'Pseudo_Al'
  density = 2700.0, ! kg/m^3
  tm_lame1 = 5.20e+10
  tm_lame2 = 2.60e+10
  phases = 'PC-aluminum_2', 'PC-aluminum', 'aluminum'
  ref_temp        = 0.0
  ref_enthalpy    = 0.0
/

&PHASE
  name = 'aluminum'
  specific_heat = 900.0,  ! J/kg-K
  conductivity = 240.0,  ! W/m-K
  tm_ref_density = 2700.0 ! kg/m^3
  tm_ref_temp = 500.0
  tm_linear_cte = 2.2e-5
/

&PHASE
  name = 'PC-aluminum'
  specific_heat = 1200.0,  ! J/kg-K
  conductivity = 270.0,  ! W/m-K
  tm_ref_density = 2690.7 ! kg/m^3
  tm_ref_temp = 400.0
  tm_linear_cte = 2.1e-5
/

&PHASE
  name = 'PC-aluminum_2'
  specific_heat = 1200.0,  ! J/kg-K
  conductivity = 270.0,  ! W/m-K
  tm_ref_density = 2712.82 ! kg/m^3
  tm_ref_temp = 350.0
  tm_linear_cte = 2.0e-5
/

&PHASE_CHANGE
  low_temp_phase = 'PC-aluminum_2'
  high_temp_phase = 'PC-aluminum'
  solid_frac_table = 350.0 1.0
                     355.0 0.8
                     360.0 0.6
                     365.0 0.4
                     370.0 0.2
                     375.0 0.0
  latent_heat =  10000.0 ! J/kg
/

&PHASE_CHANGE
  low_temp_phase = 'PC-aluminum'
  high_temp_phase = 'aluminum'
  solidus_temp  = 398.0
  liquidus_temp = 402.0
  latent_heat   =  10000.0 ! J/kg
/

&PHYSICS
  materials = 'Pseudo_Al'
  heat_transport  = T
  legacy_solid_mechanics = T
/

&DIFFUSION_SOLVER
  abs_temp_tol       = 0.0
  rel_temp_tol       = 1.0e-4
  abs_enthalpy_tol   = 0.0
  rel_enthalpy_tol   = 1.0e-4
  nlk_tol            = 1.0e-3
  nlk_preconditioner = 'hypre_amg'
/

&LEGACY_SOLID_MECHANICS
  convergence_criterion   = 1.0e-12,
  nlk_max_vectors         = 20
  nlk_vector_tolerance    = 0.01
  maximum_iterations      = 100
/

constant time increment
loose solver tolerance

&NUMERICS
    dt_init                          = 1.00e-2,
    dt_max                           = 5.0e0,
    dt_grow                          = 100,
/

Run for 100 sec

&OUTPUTS

    output_t                     = 0.0e+00, 6.0e+00,
    output_dt                    = 6.0
    output_dt_multiplier         = 1

/

&PARALLEL_PARAMETERS

    Partitioner  = 'automatic',

/
