&MESH

    Mesh_File = 'mesh.exo'

/

&OUTPUTS

    Output_T                    = 0.0, 0.25
    Output_Dt                   = 0.1
    Int_Output_Dt_Multiplier    = 0
    Short_Output_Dt_Multiplier  = 1

/

&LINEAR_SOLVER

    name                            = 'projection'
    method                          = 'fgmres'
    preconditioning_method          = 'ssor'
    preconditioning_steps           = 4
    relaxation_parameter            = 1.4
    convergence_criterion           = 1.0e-12
    maximum_iterations              = 200

/

&NUMERICS

    volume_track_interfaces     = .false.
    volume_track_brents_method  = .true.
    volume_track_iter_tol       = 1.0e-12
    projection_linear_solution  = 'projection'
    discrete_ops_type           = 'ortho'
    courant_number              = 0.5
    viscous_implicitness        = 0.0
    dt_init                     = 1.0e-03
    dt_grow                     = 1.1
    dt_min                      = 1.0e-05
    dt_max                      = 1.0

/

&PHYSICS

    body_force_density = 0.0, 0.0, 0.0
    legacy_flow            = .true.
    inviscid               = .false.
    stokes                 = .false.

/

&TURBULENCE

    length      = 0.1

/

&BC

    Surface_Name     = 'conic'
    Conic_X          = 1.0
    Conic_Constant   = 0.0
    Conic_Tolerance  = 1.0e-6
    BC_Variable      = 'pressure'
    BC_Type          = 'dirichlet'
    BC_Value         = 100000.0

/

&BC

    Surface_Name     = 'conic'
    Conic_X          = 1.0
    Conic_Constant   = -5.0
    Conic_Tolerance  = 1.0e-6
    BC_Variable      = 'pressure'
    BC_Type          = 'dirichlet'
    BC_Value         = 0.0

/

&BC

    Surface_Name     = 'conic'
    Conic_Y          = 1.0
    Conic_Constant   = 0.0
    Conic_Tolerance  = 1.0e-6
    BC_Variable      = 'velocity'
    BC_Type          = 'no-slip'

/

&BC

    Surface_Name     = 'conic'
    Conic_Y          = 1.0
    Conic_Constant   = -1.0
    Conic_Tolerance  = 1.0e-6
    BC_Variable      = 'velocity'
    BC_Type          = 'no-slip'

/

&BODY

    Material_number  = 1
    Surface_Name     = 'background'
    Velocity         = 0.0, 0.0, 0.0
    Temperature      = 0.0

/

&MATERIAL

    Material_Name           = 'water'
    Material_Number         = 1
    Material_Feature        = 'background'
    priority                = 1

    Density                 = 1.0

/

&PHASE
  name = 'water'
  property_name(1) = 'density',       property_constant(1) = 1.0
  property_name(2) = 'specific heat', property_constant(2) = 1.0
  property_name(3) = 'viscosity',     property_constant(3) = 1.0e-5
/

&MATERIAL_SYSTEM
  name = 'water'
  phases = 'water'
/

&PARALLEL_PARAMETERS

    Partitioner  = 'chaco'

/

&PROBE

   probe_name         = 'umin'
   probe_description  = 'cell at wall'
   probe_coords       = 1.25, 0.0455, 0.5
   probe_coords_scale = 1.0
/

&PROBE

   probe_name         = 'umax'
   probe_description  = 'upstream centerline'
   probe_coords       = 1.25, 0.5, 0.5
   probe_coords_scale = 1.0

/ 

&PROBE

   probe_name         = 'pdown'
   probe_description  = 'downstream centerline'
   probe_coords       = 3.75, 0.5, 0.5
   probe_coords_scale = 1.0

/ 
